@model Collection.Models.Toy

@{
    <div class="row">
        <div class="col-lg-12 line"><h4>Details</h4></div>
    </div>
    if (Model != null)
    {
        <div class="row">
            <div class="col-lg-12 line"><h4>Gallery</h4></div>

            @if (Model.Gallery != null)
                {
                    foreach (var picture in Model.Gallery)
                    {
                    <div class="card_box">
                        <div class="card_body">
                            <img  src="~/img/toys/@picture.FileName" />
                        </div>
                    </div>
                }
            }
        </div>


    }
    else
    {
        <p>Error!</p>
    }
}